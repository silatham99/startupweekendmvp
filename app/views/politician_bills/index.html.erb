<h1>Listing politician_bills</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @politician_bills.each do |politician_bill| %>
      <tr>
        <td><%= link_to 'Show', politician_bill %></td>
        <td><%= link_to 'Edit', edit_politician_bill_path(politician_bill) %></td>
        <td><%= link_to 'Destroy', politician_bill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Politician bill', new_politician_bill_path %>
